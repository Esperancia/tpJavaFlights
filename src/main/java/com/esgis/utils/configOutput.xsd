  <xs:complexType name="flux">
    <xs:sequence>
      <xs:element name="chemin">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="/Applications/FichiersNonSynchro/master_irt/m2irt/Examens/examjava/data/ouest"/>
            <xs:enumeration value="/Applications/FichiersNonSynchro/master_irt/m2irt/Examens/examJava/data/central"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="nommage">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value=".*OUEST.*"/>
            <xs:enumeration value="CENTRAL"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="frequence">
        <xs:annotation>
          <xs:documentation>expression régulières  expression régulières</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="60"/>
            <xs:enumeration value="100"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element type="xs:string" name="lot">
        <xs:annotation>
          <xs:documentation>seconds  seconds</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="traitements">
    <xs:sequence>
      <xs:element type="flux" name="flux" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
